OriginalPrice id=\"price\">{originalPrice}</OriginalPrice>\r\n        </div>\r\n        <RoundDiv>\r\n          <div onClick={addToBag} className=\"btn-round  mr-2\">\r\n            <LocalMall fontSize=\"small\" />\r\n          </div>\r\n          <div onClick={removeItem} className=\"btn-round  mr-2\">\r\n            <DeleteForever fontSize=\"small\" />\r\n          </div>\r\n        </RoundDiv>\r\n      </div>\r\n      <div className=\"rating\">\r\n        {rating} <Star style={{ fontSize: 10 }} />\r\n      </div>\r\n    </Card>\r\n  );\r\n}\r\n\r\nfunction Wishlist() {\r\n  const [{ wishlist }] = useStateValue();\r\n  const wishlistLength = wishlist.length;\r\n  return (\r\n    <Container>\r\n      <Title>\r\n        <span\r\n          style={{\r\n            position: \"absolute\",\r\n            left: \"0rem\",\r\n          }}\r\n        >\r\n          <h1>My Wishlist</h1>\r\n          <div\r\n            style={{\r\n              height: \".2rem\",\r\n              width: \"9rem\",\r\n              background: \"#fdb827\",\r\n              borderRadius: \"15%\",\r\n            }}\r\n          ></div>\r\n        </span>\r\n      </Title>\r\n      {wishlistLength === 0 ? (\r\n        <Emptybag>\r\n          <div>\r\n            <img\r\n              alt=\"duhud\"\r\n              src=\"https://i.pinimg.com/originals/f6/e4/64/f6e464230662e7fa4c6a4afb92631aed.png\"\r\n              height=\"526\"\r\n              width=\"600\"\r\n            />\r\n          </div>\r\n        </Emptybag>\r\n      ) : (\r\n        <Div>\r\n          {wishlist.map((item) => (\r\n            <CARD\r\n              key={item.id}\r\n              id={item.id}\r\n              pName={item.pName}\r\n              image={item.image}\r\n              rating={item.rating}\r\n              price={item.price}\r\n              originalPrice={item.originalPrice}\r\n            />\r\n          ))}\r\n        </Div>\r\n      )}\r\n    </Container>\r\n  );\r\n}\r\n\r\nexport default Wishlist;\r\n\r